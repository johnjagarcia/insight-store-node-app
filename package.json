{
  "name": "fort-starter",
  "version": "1.0.0",
  "description": "Starter project for fortjs using typescript",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack --config webpack.config.js",
    "webpack:watch": "webpack --config webpack.config.js --watch",
    "lint": "tslint src/**/*.ts",
    "build": "npm run webpack",
    "start": "cross-env NODE_ENV=development npm run webpack:watch",
    "deploy": "cross-env NODE_ENV=production npm run build",
    "test": "cross-env NODE_ENV=test jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ujjwalguptaofficial/fortjs-typescript-starter.git"
  },
  "author": "Ujjwal Gupta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ujjwalguptaofficial/fortjs-typescript-starter/issues"
  },
  "homepage": "https://github.com/ujjwalguptaofficial/fortjs-typescript-starter#readme",
  "dependencies": {
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "fortjs": "^1.15.0",
    "fortjs-swagger": "^1.2.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/jest": "^24.0.15",
    "@types/node": "^10.17.24",
    "@umijs/preset-react": "^1.5.5",
    "axios": "^0.19.0",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "nodemon-webpack-plugin": "^4.1.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.11.0",
    "tslint-config-security": "^1.13.0",
    "typescript": "^3.2.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-node-externals": "^1.7.2"
  },
  "project": {
    "framework": "fortjs",
    "language": "typescript"
  }
}
